{
    "name": "Data Generator",
    "lifecycle": "once",
    "workers": 1,
    "assets": ["elasticsearch"],
    "operations": [
        {
            "_op": "elasticsearch_data_generator",
            "size": 10000
        },
        {
            "_op": "elasticsearch_index_selector",
            "index": "auto-data-logs",
            "type": "events"
        },
        {
            "_op": "elasticsearch_bulk",
            "size": 100
        }
    ],
    "__metadata": {
        "cli" : {
            "cluster": "http://clusterTwo",
            "version": "0.0.1",
            "job_id": "jobId"
        }

    }
}
